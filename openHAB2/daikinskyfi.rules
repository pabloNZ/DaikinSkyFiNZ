rule "Fan Mode Change"
when
	Item heatingDaikinFan changed
then
if (heatingDaikinFan.state==1)
	executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&f=1")
	else
		if (heatingDaikinFan.state==2)
	executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&f=2")
	if (heatingDaikinFan.state==3)
executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&f=3")
if (heatingDaikinFan.state==5)
executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&f=5")
if (heatingDaikinFan.state==6)
executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&f=6")
if (heatingDaikinFan.state==7)
executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&f=7")
end

rule "Heating Mode Change"
when
	Item heatingDaikinMode changed
then
	if (heatingDaikinMode.state==1)
		executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&m=1")
		else
			if (heatingDaikinMode.state==2)
		executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&m=2")
		if (heatingDaikinMode.state==4)
	executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&m=4")
	if (heatingDaikinMode.state==8)
executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&m=8")
if (heatingDaikinMode.state==16)
executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&m=16")
if (heatingDaikinMode.state==9)
executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&m=9")
if (heatingDaikinMode.state==3)
executeCommandLine("curl http://192.168.1.17:2000/set.cgi?pass=92136&m=3")
end

